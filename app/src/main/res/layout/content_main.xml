<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/test123"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.digiota.moviereel.ui.topfive.TopFiveActivity"
    tools:showIn="@layout/app_bar_main">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:id="@+id/content_frame">
    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerview_topfive"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:paddingTop="?attr/actionBarSize"
        tools:context=".ui.topfive.TopFiveActivity"
        android:paddingBottom="0dp" />
    </FrameLayout>



</android.support.constraint.ConstraintLayout>

